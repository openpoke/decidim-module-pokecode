{
  "name": "decidim-pokecode",
  "version": "0.1.0",
  "description": "A Decidim module that adds Pokecode functionality to your Decidim application.",
  "scripts": {
    "lint": "eslint -c .eslintrc.json --no-error-on-unmatched-pattern --ignore-pattern app/packs/vendor --ext .js app/packs",
    "stylelint": "stylelint app/packs/**/*.scss",
    "stylelint-fix": "stylelint **/*.scss --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openpoke/decidim-module-pokecode.git"
  },
  "keywords": [
    "decidim"
  ],
  "author": "ivan@pokecode.net",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@decidim/browserslist-config": "^0.31.0",
    "@decidim/webpacker": "^0.31.0"
  },
  "devDependencies": {
    "@decidim/dev": "^0.31.0",
    "@decidim/eslint-config": "^0.31.0",
    "@decidim/stylelint-config": "^0.31.0"
  },
  "browserslist": ["extends @decidim/browserslist-config"]
}
